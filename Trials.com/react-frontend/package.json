        "start": "SET NODE_ENV=development && react-scripts --openssl-legacy-provider start",
        "start-prod": "SET NODE_ENV=production & react-scripts start",
        "build-dev": "SET NODE_ENV=development & react-scripts build",
        "build-prod": "SET NODE_ENV=production & react-scripts build",
        "build": "react-scripts build",
        "test": "react-scripts test --passWithNoTests",
        "eject": "react-scripts eject",
        "netlify:deploy": "netlify deploy --dir=./build -p -m \"$(git log -1 --pretty=%B)\"",
        "docker:deploy": "npm run build && npm run netlify:deploy && npm run docker:push-all",
        "docker:push-all": "npm run docker:push-react && npm run docker:push-node && npm run docker:push-nginx && npm run docker:push-sql",
        "docker:push-react": "docker push rjbradach/trialsnp-react:latest",
        "docker:push-node": "cd ../node-server && docker push rjbradach/trialsnp-node:latest",
        "docker:push-sql": "cd ../database && docker push rjbradach/trialsnp-sqldb:latest",
        "docker:push-nginx": "cd ../nginx && docker push rjbradach/trialsnp-nginx:latest"
    },
